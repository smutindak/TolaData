<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TolaData UI Automation Tests">
    <listeners>
        <listener class-name="com.toladata.utils.TestRetryAnalyzerListener"/>
        <listener class-name="com.toladata.utils.TestListener"/>
    </listeners>

    <test name="Login UI Test Cases">
        <classes>
            <class name="com.toladata.tests.logintestcases.LoginTestCases"/>
        </classes>
    </test>
    <test name="Project Phases UI Test Cases">
        <classes>
            <class name="com.toladata.tests.projectphasestestcases.PhasesTestCases"/>
        </classes>
    </test>
</suite>